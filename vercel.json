{
  "version": 2,
  "builds": [
    { "src": "frontend/package.json", "use": "@vercel/static-build" }
  ],
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://backend.example.com/api/:path*" },
    { "source": "/static/:path*", "destination": "https://backend.example.com/static/:path*" }
  ]
}
